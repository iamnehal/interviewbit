int Solution::minPathSum(vector<vector<int> > &A) {
    int m=A.size();
    int n=A[0].size();
    
    
    
    int dp[m][n];
    int i,j;
    int sum=0;
    for(i=0;i<m;i++)
    { sum+=A[i][0];
    dp[i][0]=sum;
    }
    sum=0;
    for(j=0;j<n;j++)
    {
        
        
        sum=A[0][j]+sum;
        dp[0][j]=sum;
    }
    
    for(i=1;i<m;i++)
    {
        
        for(j=1;j<n;j++)
        {
            
            dp[i][j]=A[i][j]+ min(dp[i-1][j],dp[i][j-1]);
        }
    }
    
    return dp[m-1][n-1];
    
    
}
